{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport * as d3 from 'd3';\nimport numeral from 'numeral';\n\nvar bar_chart = function bar_chart(_ref) {\n  var data = _ref.data,\n      selected_theme = _ref.selected_theme;\n  var budgetPerYear = [];\n  var million = 100000;\n\n  _.forIn(data.budgetPerYear, function (data, key) {\n    budgetPerYear.push({\n      year: key.substring(2, 4),\n      all: data.all,\n      focused: data.focused\n    });\n  });\n\n  var maxBudget = _.maxBy(budgetPerYear, 'all');\n\n  var mouseOver = function mouseOver(_, year) {\n    d3.select(\"#tooltip-budget\".concat(year)).style('visibility', 'visible');\n  };\n\n  var mouseOut = function mouseOut(_, year) {\n    d3.select(\"#tooltip-budget\".concat(year)).style('visibility', 'hidden');\n  };\n\n  return /*#__PURE__*/_jsx(\"div\", {\n    className: \"flex items-end justify-center w-full\",\n    style: {\n      height: '70px'\n    },\n    children: budgetPerYear.map(function (d, index) {\n      return /*#__PURE__*/_jsxs(\"div\", {\n        id: \"bar\",\n        className: \"relative flex justify-center mx-1 cursor-pointer\",\n        onMouseOver: function onMouseOver() {\n          return mouseOver(_, d.year);\n        },\n        onMouseOut: function onMouseOut() {\n          return mouseOut(_, d.year);\n        },\n        style: {\n          background: 'rgba(0, 0, 0, 0.2)',\n          borderRadius: '3px',\n          width: '30px',\n          height: \"\".concat(d.all / maxBudget.all * 100, \"%\")\n        },\n        children: [/*#__PURE__*/_jsx(\"div\", {\n          id: \"in-bar\",\n          className: \"absolute bottom-0 pointer-events-none\",\n          style: {\n            backgroundColor: selected_theme.color,\n            borderRadius: '3px',\n            width: '30px',\n            height: \"\".concat(d.focused / d.all * 100, \"%\")\n          }\n        }), /*#__PURE__*/_jsx(\"div\", {\n          id: \"text-in-bar\",\n          className: \"absolute pointer-events-none p2\",\n          style: {\n            bottom: '-25px'\n          },\n          children: d.year\n        }), /*#__PURE__*/_jsxs(\"div\", {\n          id: \"tooltip-budget\".concat(d.year),\n          className: \"absolute invisible font-bold \",\n          style: {\n            bottom: '90%',\n            left: '70%',\n            width: '160px'\n          },\n          children: [/*#__PURE__*/_jsxs(\"div\", {\n            id: \"tooltip-budget-header\",\n            className: \"px-2 py-1 rounded-t-md text-white-default p2\",\n            style: {\n              backgroundColor: selected_theme.color\n            },\n            children: [\"\\u0E1B\\u0E35 25\", d.year]\n          }), /*#__PURE__*/_jsxs(\"div\", {\n            id: \"tooltip-budget-body\",\n            className: \"p-2 rounded-b-md p3\",\n            style: {\n              backgroundColor: selected_theme.color50\n            },\n            children: [/*#__PURE__*/_jsxs(\"span\", {\n              children: [/*#__PURE__*/_jsx(\"p\", {\n                children: \"\\u0E07\\u0E1A\\u0E17\\u0E31\\u0E49\\u0E07\\u0E2B\\u0E21\\u0E14\\u0E43\\u0E19\\u0E1B\\u0E35\"\n              }), /*#__PURE__*/_jsxs(\"p\", {\n                style: {\n                  color: selected_theme.text_color\n                },\n                children: [numeral(d.all / million).format('0,0'), \" \\u0E25\\u0E49\\u0E32\\u0E19\\u0E1A\\u0E32\\u0E17\"]\n              })]\n            }), /*#__PURE__*/_jsxs(\"span\", {\n              children: [/*#__PURE__*/_jsx(\"p\", {\n                children: \"\\u0E07\\u0E1A\\u0E40\\u0E09\\u0E1E\\u0E32\\u0E30\\u0E40\\u0E23\\u0E37\\u0E48\\u0E2D\\u0E07\"\n              }), /*#__PURE__*/_jsxs(\"p\", {\n                style: {\n                  color: selected_theme.text_color\n                },\n                children: [d.year / million >= 0 ? d.year / million : numeral(d.year / million).format('0,0'), ' ', \"\\u0E25\\u0E49\\u0E32\\u0E19\\u0E1A\\u0E32\\u0E17\"]\n              })]\n            })]\n          })]\n        })]\n      }, index);\n    })\n  });\n};\n\nexport default bar_chart;","map":null,"metadata":{},"sourceType":"module"}